<?xml version="1.0" encoding="utf-8"?>
		<LinearLayout android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android" 
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:background="@drawable/tianxie"
			android:padding="20dp">




			<!-- 第1线性布局 -->
			<LinearLayout android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="wrap_content">

				<!-- 表格布局 -->
				<TableLayout android:orientation="vertical"
					android:layout_width="fill_parent" android:layout_height="wrap_content">

					<TableRow>
					<!--  
						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="遗迹名称:"
							android:textSize="25px" />
						<EditText android:layout_width="50px"
							android:layout_height="wrap_content"
							android:id="@+id/grimpObserveName"
							></EditText>
							-->
						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="调查时间:"
							android:textSize="25px"  />

						<TableLayout android:layout_width="wrap_content"
							android:layout_height="wrap_content">
							<TableRow>
								<TextView android:id="@+id/dateDisplay"
									android:paddingTop="15px" android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:layout_gravity="center_horizontal"
									android:textSize="25px" />
								<Button android:id="@+id/button" android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="设置日期" />
							</TableRow>
						</TableLayout>

					</TableRow>
					<TableRow>



						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="地区(重庆):"
							android:textSize="25px" />

						<Spinner android:id="@+id/areaObserve"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:prompt="@string/dic"
							android:entries="@array/text">
						</Spinner>

						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="地理位置:"
							android:textSize="25px" android:paddingLeft="15px" />
						<EditText android:layout_width="50px"
							android:layout_height="wrap_content"
							android:id="@+id/geographicalObservePosition"
							></EditText>



					</TableRow>

					<TableRow>
						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="遗迹点坐标N:"
							android:textSize="25px" android:paddingTop="15px" />

						<TableLayout android:orientation="vertical"
							android:layout_width="wrap_content" android:layout_height="wrap_content">
							<TableRow>
								<TextView android:layout_width="60px"
									android:id="@+id/foundationSpendN"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:paddingLeft="15px"
								
									android:editable="false"
									></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="度"
									android:textSize="25px"
									android:paddingTop="15px" />
								<TextView android:layout_width="60px"
									android:id="@+id/foundationMinuteN"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:paddingLeft="15px"
									android:editable="false"></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="分"
									android:textSize="25px" />
								<TextView android:layout_width="60px"
									android:id="@+id/foundationSecondN"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:paddingLeft="15px"
									android:editable="false"></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="秒"
									android:textSize="25px" />
							</TableRow>
						</TableLayout>

						<TextView android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:text="遗迹点坐标E"
							android:textSize="25px" android:paddingLeft="15px" />
						<TableLayout android:orientation="vertical"
							android:layout_width="wrap_content" android:layout_height="wrap_content">
							<TableRow>
							<TextView android:layout_width="60px"
									android:id="@+id/foundationSpendE"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:paddingLeft="15px"
									android:editable="false"></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="度"
									android:textSize="25px"
									android:paddingTop="15px" 
									/>
								<TextView android:layout_width="60px"
									android:id="@+id/foundationMinuteE"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:paddingLeft="15px"
									android:editable="false"></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="分"
									android:textSize="25px" />
								<TextView android:layout_width="60px"
									android:id="@+id/foundationSecondE"
									android:paddingLeft="15px"
									android:layout_height="wrap_content" android:numeric="integer"
									android:text="00"
									android:editable="false"></TextView>
								<TextView android:layout_width="wrap_content"
									android:layout_height="wrap_content" android:text="秒"
									android:textSize="25px" />
								
							</TableRow>
						
						</TableLayout>
					</TableRow>
					<TableRow>
								<Button
									android:id="@+id/getMap"
									android:layout_width="wrap_content"
									android:layout_height="60dp"
									android:text="地图"/>	
							</TableRow>
				</TableLayout>
			</LinearLayout>


			<!-- 第2线性布局 -->
			<LinearLayout android:orientation="vertical"
				android:layout_width="wrap_content" android:layout_height="wrap_content">


			</LinearLayout>

		</LinearLayout>
