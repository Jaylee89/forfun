<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="40dp"
    android:paddingRight="40dp"
    >
    <TableLayout 
    android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:id="@+id/getback_string_input"
	>
	<TableRow>
    <TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="文件路径:"
			android:padding="30dp"
		></TextView>
		<TextView
			android:id="@+id/getback_path"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_weight="7"
			android:text=""
			/>
			<Button
			android:id="@+id/skin_file_btn1"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:text="浏览"
			android:layout_weight="3"
		></Button>
		</TableRow>
		
		</TableLayout>
	<TableLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@id/getback_string_input"
		android:stretchColumns="*"
	>
	<TableRow>
		<Button
			android:id="@+id/set_getback_btn"
		    android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:text="还原"
		>
		</Button>
		<Button
			android:id="@+id/set_getback_cancel_btn"
		    android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:text="取消"
		>
		</Button>
		</TableRow>
	</TableLayout>
</RelativeLayout>
